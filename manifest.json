{

  "manifest_version": 2,
  "name": "goodbye-seekbar",
  "version": "1.0",

  "description": "Hide seekbar on YouTube",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["main.js"]
    }
  ],

  "applications": {
    "gecko": {
      "id": "extension.goodbye-seekbar@sweshelo.jp"
    }
  }

}
